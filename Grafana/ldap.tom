[[servers]]
host = ""
port = 389
use_ssl = false
start_tls = false
ssl_skip_verify = false
bind_dn = "\\%s"
search_filter = "(sAMAccountName=%s)"
search_base_dns = ["dc=,dc=corp"]

[servers.attributes]
name = "givenName"
surname = "sn"
username = "sAMAccountName"
member_of = "memberOf"
email =  "mail"
